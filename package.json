{
    "name": "codecept",
    "version": "1.0.0",
    "description": "Codecept basic template",
    "license": "ISC",
    "author": "Donald Navarro",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "test:run": "codeceptjs run --plugins allure",
        "report:generate": "allure generate output/allure --clean -o output/allure-report",
        "report:open": "allure open output/allure-report",
        "test": "npm-run-all -c --serial test:run report:generate report:open"
    },
    "devDependencies": {
        "@codeceptjs/allure-legacy": "^1.0.2",
        "allure-commandline": "^2.34.0",
        "codeceptjs": "^3.7.3",
        "playwright": "^1.53.0"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5"
    }
}
